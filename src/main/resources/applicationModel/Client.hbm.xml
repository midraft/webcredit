<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


    <class name= "com.example.application.model.Clients" table="CLIENTS">
        <id name="CLIENTID" column="CLIENTID"/>
        <property name="SURNAME" column="SURNAME"/>
        <property name="NAME" column="NAME"/>
        <property name="PATRONYMIC" column="PATRONYMIC"/>
        <property name="PHONE" column="PHONE"/>
        <property name="email" column="EMAIL"/>
        <property name="PASSPORTSERIES" column="PASSPORTSERIES"/>
        <property name="PASSPORTID" column="PASSPORTID"/>
        <property name="creditAmount" column="credit_amount"/>
        <property name="creditTerm" column="credit_term"/>
        <property name="entryInterestRate" column="entry_interest_rate"/>
    </class>

</hibernate-mapping>